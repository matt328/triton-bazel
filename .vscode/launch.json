{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb-dap",
      "request": "launch",
      "name": "(DAP) Launch)",
      "stopOnEntry": false,
      "program": "${workspaceFolder}/bazel-bin/util-lib/util-lib_linux",
      "args": [],
      "env": [],
      "cwd": "${workspaceRoot}"
    },
    {
      "name": "(gdb) Launch",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/bazel-bin/util-lib/util-lib_linux",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    },
    {
      "type": "lldb",
      "request": "launch",
      "stopOnEntry": false,
      "name": "(LLDB) Launch",
      "program": "${workspaceFolder}/bazel-bin/util-lib/util-lib_linux",
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "util-lib (Windows)",
      "type": "cppvsdbg",
      "request": "launch",
      "program": "${workspaceRoot}\\bazel-bin\\util-lib\\util-lib.exe",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
    }
  ]
}